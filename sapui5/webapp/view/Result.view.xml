<mvc:View xmlns:table="sap.ui.table"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    controllerName="egb.sapui5.controller.Result"
    displayBlock="true">
    <Page title="Resultado"
        showNavButton="true"
        navButtonPress="onNavBack">
        <Text text="TABLA DE RESULTADOS"/>
        <sap.ui.comp.smarttable:SmartTable xmlns:sap.ui.comp.smarttable="sap.ui.comp.smarttable"
            xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
            width="100%"
            direction="Column"
            fitContainer="true"
            tableType="ResponsiveTable"
            header="Smart Table"
            enableAutoBinding="true"
            id="table0"
            cd:w5g.dt.context="/Products"
            entitySet="Products"
            initiallyVisibleFields="ProductID,ProductName,CategoryID,QuantityPerUnit,ReorderLevel"/>
        <content/>
    </Page>
</mvc:View>